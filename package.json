{
  "name": "three-space",
  "version": "1.0.0",
  "siteRoot": "three-space",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -Rf dist && rm -Rf dist-discard && rm -Rf scss-dest",
    "build": "node-sass scss --output scss-dest --output-style compressed && node-sass base-scss --output scss-dest --output-style compressed && webpack --config webpack.prod.js",
    "build:test": "webpack --config webpack.dev.js",
    "build:watch": "webpack --config webpack.dev.js --watch",
    "license-gen": "webpack --config webpack.license-gen.js",
    "start": "webpack serve --config webpack.dev.js",
    "lint:js-one": "eslint",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:css": "stylelint 'scss/**/*.scss'",
    "lint:html": "htmlhint 'html/index.html'",
    "build-css": "node-sass scss --output scss-dest --output-style compressed && node-sass base-scss --output scss-dest --output-style compressed"
  },
  "author": "Tomoki Sato",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "expect.js": "^0.3.1",
    "htmlhint": "^0.14.2",
    "mocha": "^8.2.1",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^5.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-license-plugin": "^4.1.2",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@pixiv/three-vrm": "^0.5.6",
    "three": "^0.124.0"
  }
}
